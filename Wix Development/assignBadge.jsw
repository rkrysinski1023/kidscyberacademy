import { badges } from 'wix-members-backend';

export function badgeAssign(badgeId, currentUserId) {

    const memberIds = [currentUserId];

  return badges.assignMembers(badgeId, memberIds)
    .then((assignedMembers) => {
      return assignedMembers;
    })
    .catch((error) => {
      console.error(error);
    });
}

